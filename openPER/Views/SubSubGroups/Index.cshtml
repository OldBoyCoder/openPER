@model openPER.ViewModels.SubSubGroupsViewModel

@{
    ViewData["Title"] = "Index";
}

<h1>SubSubGroupsView</h1>

<div>
    <h4>SubSubGroups</h4>
    <hr />
    <table class="table table-sm">
    @foreach (var group in Model.SubSubGroups)
    {
        <tr><td>@group.Code</td><td>@group.Narrative</td></tr>
    }
    </table>
</div>
