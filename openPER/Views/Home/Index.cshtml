@model IEnumerable<openPER.Models.MakeViewModel>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">


    <h1 class="display-4">Wotcha</h1>

<h1>Makes</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.ActionLink(item.Description, "Models", "Table", new {Make=item.Code})
            </td>

        </tr>
}
    </tbody>
</table>
    	
</div>
