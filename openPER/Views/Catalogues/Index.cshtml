@model openPER.ViewModels.CataloguesViewModel

@{
    ViewData["Title"] = $"Catalogues for {Model.Navigation.Breadcrumb.MakeDescription} {Model.Navigation.Breadcrumb.ModelDescription}";
}
@await Html.PartialAsync("_Breadcrumb", Model.Navigation)
<div class="container-fluid">
    <div class="row">
        <div class="col-lg-4 col-md-4 col-sm-12 col-12">
            @await Html.PartialAsync("_SidebarMenu", Model.Navigation)
        </div>
        <div class="col-lg-8 col-md-8 col-sm-12 col-12">
            <img src="@Model.ImagePath" alt="@(Model.Navigation.Breadcrumb.ModelDescription)" title="@(Model.Navigation.Breadcrumb.ModelDescription)">

        </div>
    </div>
</div>




