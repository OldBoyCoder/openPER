@using openPER.ViewModels
@model MakesViewModel

@{
    ViewData["Title"] = "Index";
}

<h1>Makes</h1>

<div>
    <h4>MakesViewModel</h4>
    <table class="table">
    @foreach (var make in Model.Makes)
    {
        <tr><td>@make.Code</td><td>
@Html.ActionLink(make.Description, "Index", "Models", new { Model.ReleaseCode, MakeCode=make.Code})</td></tr>
    }
    </table>
</div>
<div>
</div>
